<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:ModernDashboard.MVVM.ViewModels"
                    xmlns:views="clr-namespace:ModernDashboard.MVVM.Views">
    <!--Data Templates apply Views to ViewModel objects displayed in the UI-->
    
    <!--Startup Page-->
    <DataTemplate DataType="{x:Type views:DesktopView}">
        <views:DesktopView/>
    </DataTemplate>
</ResourceDictionary>